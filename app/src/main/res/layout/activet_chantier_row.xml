<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.CardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:id="@+id/vue_liste_c"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    card_view:cardCornerRadius="5dp"
    card_view:cardElevation="1dp"
    card_view:cardUseCompatPadding="true"
    android:background="#F8F8F8"
    >



    <ImageView
        android:id="@+id/rectangle_5"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="#F8F8F8"
         />

    <TextView
        android:id="@+id/titre_rappo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="56dp"
        android:layout_marginTop="10dp"
        android:layout_alignParentTop="true"
        android:text="Titre Rapport Journalier 1"
        android:textAppearance="@style/titre_rappo"
        />

    <TextView
        android:id="@+id/date_rappo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="56dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="35dp"
        android:text="10/12/2018"
        android:textAppearance="@style/some_id"/>

    <ImageView
        android:id="@+id/ingenieur1"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="320dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="10dp"
        android:background="@drawable/ingenieur1" />



</android.support.v7.widget.CardView>